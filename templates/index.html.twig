      </div>
      <header class="header">
        <div class="header__inner"><a href="/" class="header__logo">
            <svg class="icon icon--logo">
              <use xlink:href="#logo"></use>
            </svg></a>
          <div class="header__menu">
            <div data-target="body" data-direct="true" data-class="menu-is-open" class="menu-trigger js-class-toggler">
              <div class="menu-trigger__burger"><span></span><span></span><span></span></div>
              <div class="menu-trigger__icon">
                <svg class="icon icon--back">
                  <use xlink:href="#back"></use>
                </svg>
              </div>
            </div>
            <nav class="menu menu--header">
              <div class="menu__inner">
                <div class="menu__items">
                    {% if list|length %}
                        {% for block in list %}
                          <div class="menu__item"><a href="/#{{ block.alias }}" data-menuanchor="{{ block.alias }}">{{ block.name }}</a></div>
                        {% endfor %}
                    {% endif %}
                </div>
              </div>
            </nav>
          </div>
          <div class="header__discuss">
            <button type="button" data-modal="modal--discuss" class="btn btn--default">Обсудить проект</button>
          </div>
          <div class="header__social">
            <div class="header__social-inner">
              <div class="header__social-items">
                <div class="header__social-item">
                  <button type="button" class="btn btn--social">
                    <svg class="icon icon--vk">
                      <use xlink:href="#vk"></use>
                    </svg>
                  </button>
                </div>
                <div class="header__social-item">
                  <button type="button" class="btn btn--social">
                    <svg class="icon icon--telegram">
                      <use xlink:href="#telegram"></use>
                    </svg>
                  </button>
                </div>
                <div class="header__social-item">
                  <button type="button" class="btn btn--social">
                    <svg class="icon icon--instagram">
                      <use xlink:href="#instagram"></use>
                    </svg>
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <section class="content">
        <div class="sections sections--main js-init-fp">
          {% if list|length %}
              {% for block in list %}
                {% if block.alias == "main" %}
                  <section class="section section--main section--slider section--slider-no-arrows">
                    <div class="section__inner">
                      <div class="section__slider-wrapper">
                        <div class="section__slider section__slider--no-arrows js-main-slider">
                          {% for item in block.data.slides %}
                            <div style="background-image: url('http://hr-engine-backend.ams.kube.xbet.lan/genfiles/{{ item.image }}');" class="section__slider-item">
                              <div class="section__slide-inner">
                                <div class="section__slide-text">{{ item.title}} <span class="typable typable--blue"><span data-strings="{{ item.sub_title}}" class="typable__text js-type-it"></span><span class="typable__text typable__text--dim"></span></span>{{ item.sub_title2}}</div>
                                <div class="section__slide-action"><a href="#" data-modal="modal--discuss" class="btn btn--big">{{ item.btn_text}}</a></div>
                              </div>
                            </div>
                          {% endfor %}
                        </div>
                      </div>
                    </div>
                  </section>
                {% elseif block.alias == "company" %}
                  <section class="section section--company">
                    <div class="section__inner">
                      <div data-transformation="slider" data-breakpoint="1024px" class="section__items">
                        {% for item in block.data.advantages %}
                          <div class="section__item">
                            <div class="section__item-digits">{{ item.title }}</div>
                            <div class="section__item-text">{{ item.sub_title }}</div>
                          </div>
                        {% endfor %}
                      </div>
                      <div class="section__text">
                        {{ block.data.description|raw }}
                        <div class="section__action"><a href="#" class="btn btn--glass-white">{{ block.data.btn1_text }}</a><a href="#" data-modal="modal--connect" class="btn btn--small-fs">{{ block.data.btn2_text }}</a></div>
                      </div>
                    </div>
                  </section>
                {% elseif block.alias == "services" %}
                  <section class="section section--services">
                    <div class="section__inner">
                      <div class="section__title">
                        <div class="section__title-text">{{ block.name }}</div>
                      </div>
                      <div data-transformation="slider" data-breakpoint="1366px" data-name="services" class="section__items">
                        {% for item in block.data.services %}
                          <div class="section__item">
                            <div class="section__item-inner">
                              <div class="section__item-title">{{ item.title }}</div>
                              <div class="section__item-text">{{ item.desc }}</div>
                              <div class="section__item-action"><a href="{{ item.btn_link }}" class="btn btn--square">{{ item.btn_text }}</a></div>
                            </div>
                          </div>
                        {% endfor %}
                      </div>
                    </div>
                  </section>
                {% elseif block.alias == "vacancies" %}
                    <section class="section section--vacancies section--slider section--slider-arrows-right">
                      <div class="section__inner">
                        <div class="section__title">
                          <div class="section__title-text">{{ block.name }}</div>
                        </div>
                        <div class="section__slider-wrapper js-hide-elem">
                          <div class="section__slider section__slider--vacancies js-slider-vacancies">
                            {% for item in block.data.vacancies_group %}
                            <div class="section__slider-item">
                              <div class="section__slide-inner">
                                <div class="section__items">
                                  <div class="section__items-title">{{ item.description }}</div>
                                  {% for sub_item in item.vacancies %}
                                    <div class="item"><a href="{{ sub_item.link }}" target="_blank" class="lnk lnk--overlay"></a>
                                      <div class="item__content">
                                        <div class="item__head">
                                          <div class="item__name">{{ sub_item.title }}</div>
                                          <div class="item__info">{{ sub_item.salary }}</div>
                                        </div>
                                        <div class="item__text">
                                          {{ sub_item.desc|raw }}
                                        </div>
                                        <div class="item__action"><a href="{{ sub_item.link }}" target="_blank" class="btn btn--bsa"><span class="btn__text">Подробнее</span></a></div>
                                      </div>
                                    </div>
                                  {% endfor %}
                                </div>
                                <div class="section__media">
                                  <div class="slider section__slider--no-arrows js-slider-media">
                                    {% for image_element in block.data.vacancies_group %}
                                      <img src="http://hr-engine-backend.ams.kube.xbet.lan/genfiles/{{ image_element.image }}" alt="Удобный рабочий график" class="section__image">
                                    {% endfor %}
                                  <div class="section__action">
                                    <button type="button" data-id="0" class="btn btn--big js-open-item">Узнать больше</button>
                                  </div>
                                </div>
                              </div>
                              </div>
                            </div>
                            {% endfor %}
                          </div>
                          <div class="section__slider-arrows"></div>
                        </div>
                      </div>
                    </section>
                  {% elseif block.alias == "contacts" %}
                    <section class="section section--contacts">
                      <div class="section__inner">
                        <div class="section__title">
                          <div class="section__title-text">{{ block.name }}</div>
                        </div>
                        <div class="section__items">
                          <div class="section__item">
                            <div class="section__item-title">Юридический адрес:</div>
                            <div class="section__item-content">{{ block.data.address }}</div>
                          </div>
                          <div class="section__item">
                            <div class="section__item-title">E-mail:</div>
                            <div class="section__item-content"><a href="mailto:{{ block.data.email }}" class="lnk lnk--mail">{{ block.data.email }}</a></div>
                          </div>
                        </div>
                        <div class="section__content">
                          <div class="section__map">
                            <div id="map"></div>
                          </div>
                          <div class="section__form">
                            <form class="form form--question">
                              <div class="form__row">
                                {% for item in block.data.my_form.items %}
                                  {% if item.field_name == "input" %}
                                    <div class="form__element">
                                      <div class="form__element-control">
                                        <input type="{{ item.type }}" placeholder="{{item.placeholder}}" class="input">
                                      </div>
                                      <div class="form__element-error">текст ошибки</div>
                                    </div>
                                  {% endif %}
                                {% endfor %}
                              </div>
                              <div class="form__row">
                                {% for item in block.data.my_form.items %}
                                  {% if item.field_name == "textarea" %}
                                    <div class="form__element">
                                      <div class="form__element-control">
                                        <textarea placeholder="{{ item.placeholder }}" class="textarea"></textarea>
                                      </div>
                                      <div class="form__element-error">текст ошибки</div>
                                    </div>
                                  {% endif %}
                                {% endfor %}
                              </div>
                              <div class="form__submit">
                                <button type="submit" class="btn btn--square-green">Отправить</button>
                              </div>
                            </form>
                          </div>
                          <div class="section__offer">
                            <div class="section__offer-text">Вы хотите обсудить с нами свой проект?</div>
                            <div class="section__offer-action"><a href="#" data-modal="modal--discuss" class="btn btn--big">Обсудить проект</a></div>
                          </div>
                        </div>
                        <div class="section__footer">
                          <div class="section__footer-inner">
                            <div class="section__footer-video">
                              <div href="https://player.vimeo.com/video/346036397?title=0&amp;portrait=0&amp;byline=0&amp;autoplay=1&amp;loop=1" class="video video--white fancybox-media">
                                <svg class="icon icon--video">
                                  <use xlink:href="#video"></use>
                                </svg>
                              </div>
                            </div>
                            <div class="section__footer-text">© 2020 4people. Все права защищены</div>
                            <div class="section__footer-link"><a href="#" class="lnk lnk--white">Политика конфиденциальности</a></div>
                          </div>
                        </div>
                      </div>
                    </section>
                  {% endif %}
              {% endfor %}
          {% endif %}
        </div>
        <div href="https://player.vimeo.com/video/346036397?title=0&amp;portrait=0&amp;byline=0&amp;autoplay=1&amp;loop=1" class="video video--main fancybox-media">
          <svg class="icon icon--video">
            <use xlink:href="#video"></use>
          </svg>
        </div>
      </section>
    </div>
    <div class="modal modal--discuss">
      <div class="modal__inner">
        <div class="modal__content">
          <div class="modal__close js-modal-close">
            <svg class="icon icon--close icon_close-popup">
              <use xlink:href="#close"></use>
            </svg>
          </div>
          <div class="modal__title"><span>Обсудить проект</span></div>
          <div class="modal__form">
            <form class="form form--modal">
              <div class="form__row">
                <div class="form__element">
                  <div class="form__element-control">
                    <input type="text" placeholder="Имя" class="input">
                  </div>
                  <div class="form__element-error">текст ошибки</div>
                </div>
                <div class="form__element">
                  <div class="form__element-control">
                    <input type="tel" placeholder="Телефон" class="input">
                  </div>
                  <div class="form__element-error">текст ошибки</div>
                </div>
                <div class="form__element">
                  <div class="form__element-control">
                    <input type="email" placeholder="E-mal" class="input">
                  </div>
                  <div class="form__element-error">текст ошибки</div>
                </div>
              </div>
              <div class="form__row">
                <div class="form__element">
                  <div class="form__element-control">
                    <textarea placeholder="Ваш комментарий" class="textarea"></textarea>
                  </div>
                  <div class="form__element-error">текст ошибки</div>
                </div>
              </div>
              <div class="form__submit">
                <button type="submit" class="btn btn--square-green">Отправить</button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <div class="modal modal--request">
      <div class="modal__inner">
        <div class="modal__content">
          <div class="modal__close js-modal-close">
            <svg class="icon icon--close icon_close-popup">
              <use xlink:href="#close"></use>
            </svg>
          </div>
          <div class="modal__title"><span>Оставить заявку</span></div>
          <div class="modal__form">
